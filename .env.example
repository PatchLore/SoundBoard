# Stream Soundboard Configuration
# Demo Mode Configuration (No API keys required)
REACT_APP_DEMO_MODE=true
REACT_APP_APP_NAME=Stream Soundboard

# Manual Upload Configuration
REACT_APP_MAX_UPLOAD_SIZE=50
REACT_APP_ALLOWED_FORMATS=mp3,wav,ogg,m4a

# Optional: Future AI Integration (when you add API keys)
# REACT_APP_AI_SERVICE=your_ai_service_name
# REACT_APP_AI_API_KEY=your_ai_api_key_here

# Note: The app runs in Demo Mode by default
# Demo Mode supports manual uploads and local audio files
# No external API keys are required for basic functionality
